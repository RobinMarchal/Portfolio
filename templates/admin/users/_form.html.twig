{{ form_start(form) }}
<div class="row">
    <div class="col-md-7">
        {{ form_row(form.username) }}
    </div>
</div>
<div class="row">
    <div class="col-md-7">
        {{ form_row(form.password) }}
    </div>
</div>
<div class="row">
    <div class="col-md-7">
            {{ form_row(form.roles) }}
        
    </div>
</div>
<hr>

    {{ form_widget(form) }}
    <button class="btn btn-fonce waves-effect waves-light">{{ button|default('Enregistrer')}}</button>
{{ form_end(form) }}